        overflowToDisk="false"   
        diskPersistent="false"         
        timeToIdleSeconds="0"   
        timeToLiveSeconds="3600"        
        memoryStoreEvictionPolicy="LRU" /> 
    
    <cache name="consumerHomeCache" eternal="false"         
        maxElementsInMemory="100"   
        overflowToDisk="false"   
        diskPersistent="false"         
        timeToIdleSeconds="0"   
        timeToLiveSeconds="3600"        
        memoryStoreEvictionPolicy="LRU" />  
        
    <cache name="producerHomeCache" eternal="false"         
        maxElementsInMemory="100"   
        overflowToDisk="false"   
        diskPersistent="false"         
        timeToIdleSeconds="0"   
        timeToLiveSeconds="3600"        
        memoryStoreEvictionPolicy="LRU" />   
          
</ehcache>  